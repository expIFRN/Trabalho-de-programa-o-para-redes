import os

caminho_pasta = os.path.expanduser('~/Desktop/TRABALHO')

for nome_arquivo in os.listdir(caminho_pasta):
    if nome_arquivo.endswith('.conf') and not nome_arquivo.endswith('_old.conf'):
        caminho_antigo = os.path.join(caminho_pasta, nome_arquivo)
        
        novo_nome = nome_arquivo[:-5] + '_old.conf'  # Remove ".conf" e adiciona "_old.conf"
        caminho_novo = os.path.join(caminho_pasta, novo_nome)

        os.rename(caminho_antigo, caminho_novo)
        print(f'Renomeado: {nome_arquivo} -> {novo_nome}')
